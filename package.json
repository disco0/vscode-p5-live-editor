{
  "name": "p5-live-editor",
  "displayName": "p5js live editor",
  "description": "Live preview for p5js projects",
  "version": "0.1.0",
  "publisher": "andreapollini",
  "engines": {
    "vscode": "^1.26.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",

  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^6.14.7",
    "mocha": "^5.2.0",
    "typescript": "^2.9.2",
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "@types/p5": "^0.9.0",
    "jshint": "^2.11.0",
    "recast": "^0.18.7",
    "ws": "^6.2.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProfAndreaPollini/vscode-p5-live-editor"
  }

}