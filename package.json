{
  "name": "p5-live-editor",
  "displayName": "p5js live editor",
  "description": "Live preview for p5js projects",
  "version": "0.4.0",
  "publisher": "andreapollini",
  "engines": {
    "vscode": "^1.42.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [{
        "id": "p5-live-editor-projects",
        "title": "P5",
        "icon": "media/main_icon_e_dark.svg"
      }]
    },
    "views": {
      "p5-live-editor-projects": [{
        "id": "p5-projects-view",
        "name": "P5.js Projects"
      }]
    },
    "commands": [{
        "command": "extension.showP5LiveEditorCanvas",
        "title": "Open Preview",
        "icon": {
          "light": "media/preview.svg",
          "dark": "media/preview.svg"
        }
      },
      {
        "command": "extension.createp5project",
        "title": "New Project",
        "icon": {
          "light": "media/add.svg",
          "dark": "media/add.svg"
        }
      },
      {
        "command": "extension.deleteProject",
        "title": "Delete This Project",
        "icon": {
          "light": "$(alert)",
          "dark": "$(alert)"
        }
      },
      {
        "command": "extension.selectProject",
        "title": "Open Project",
        "icon": {
          "light": "$(alert)",
          "dark": "$(alert)"
        }
      }
    ],
    "menus": {
      "view/title": [{
        "command": "extension.createp5project",
        "when": "view == p5-projects-view",
        "group": "navigation"
      }],
      "view/item/context": [{
        "command": "extension.selectProject",
        "when": "view == p5-projects-view"
      }, {
        "command": "extension.deleteProject",
        "when": "view == p5-projects-view"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^6.14.7",
    "mocha": "^7.1.1",
    "typescript": "3.8.3",
    "vscode": "1.1.36"
  },
  "dependencies": {
    "@types/p5": "^0.9.0",
    "jsdom": "^16.2.1",
    "jshint": "^2.11.0",
    "minimist": "^1.2.3",
    "mkdirp": "1.0.3",
    "recast": "^0.18.7",
    "ws": "^6.2.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProfAndreaPollini/vscode-p5-live-editor"
  }
}